.imagoslider(
    ng-class="[conf.animation, action]"
    ng-swipe-left="goNext($event)"
    ng-swipe-right="goPrev($event)"
    )
  .prev(ng-show="conf.enablearrows"
        ng-click="goPrev($event)"
        stop-propagation)
  .next(ng-show="conf.enablearrows"
        ng-click="goNext($event)"
        stop-propagation)